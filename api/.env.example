# FastAPI Environment Configuration
# Copy to .env and fill in actual values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Model Configuration
HF_MODEL_ID=nateraw/food

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Optional: API Configuration
PORT=8000
HOST=0.0.0.0

# AI-Powered Health Tips Configuration
ENABLE_AI_TIPS=true
AI_TIP_PROVIDER=openai

# OpenAI API Key (for AI-powered tips)
OPENAI_API_KEY=your-openai-key-here

# HuggingFace API Key (alternative free option)
HF_API_KEY=your-hf-token-here